#ifndef CENSISTA_H_
#define CENSISTA_H_

typedef struct{
    int diaNacimiento;
    int mesNacimiento;
    int anioNacimiento;
}Fecha;

typedef struct{
    char direccion[50];
    struct Fecha fechaNacimiento;
}Datos;

typedef struct{
    int idCensista;
    char nombre[50];
    char apellido[50];
    int edad;
    int estado;
 //   struct Datos datoComplementario;
}Censista;

#endif /* CENSISTA_H_ */
