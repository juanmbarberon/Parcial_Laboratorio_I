#ifndef CENSISTA_H_
#define CENSISTA_H_

 struct Fecha{
    int diaNacimiento;
    int mesNacimiento;
    int anioNacimiento;
};

struct Datos{
    char direccion[50];
    struct Fecha fechaNacimiento;
};

typedef struct {
    int idCensista;
    char nombre[50];
    char apellido[50];
    int edad;
    int estado;
    struct Datos datoComplementarios;
}Censista;



#endif /* CENSISTA_H_ */
